<div class="container body-content">
    <h1 class="centered">@ViewBag.title</h1>

</div>
    @{
        //TODO #1: Use a loop to display job results in a table. Complete.
    }
   
<div>
    <table class="job-listing">
        @foreach(var job in ViewBag.jobs)
        {
        <tr>
                
                
                <td>
                    <p>ID: @job.Id</p>
                    <p>Name: @job.Name</p>
                    <p>Employer: @job.Employer</p>
                    <p>Location: @job.Location</p>
                    <p>Position Type: @job.PositionType</p>
                    <p>Skill: @job.CoreCompetency</p>
                </td>
                    
                   
                
        </tr>
            
         }
    </table>
            
            














</div>